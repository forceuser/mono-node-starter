{
	"name": "mono-node-starter",
	"version": "1.2.0",
	"description": "",
	"main": "src/index.js",
	"type": "module",
	"scripts": {
		"test": "vitest run --coverage",
		"eslint": "eslint .",
		"eslint-fix": "eslint --fix .",
		"release": "git add . && git commit -m",
		"postrelease": "git push --all",
		"bump": "npm --no-git-tag-version version patch",
		"bump-minor": "npm version minor --no-git-tag-version",
		"bump-major": "npm version major --no-git-tag-version",
		"bump-rc": "npm version prerelease --preid rc --no-git-tag-version",
		"bump-beta": "npm version prerelease --preid beta --no-git-tag-version",
		"bump-alpha": "npm version prerelease --preid alpha --no-git-tag-version"
	},
	"exports": {
		".": "./src/index.js",
		"./*": "./src/*"
	},
	"imports": {
		"#root/*": {
			"default": "./src/*"
		},
		"#logger/*": {
			"default": "./src/logger/*"
		},
		"#svc/*": {
			"default": "./src/svc/*"
		},
		"#config/*": {
			"default": "./src/config/*"
		},
		"#proxy/*": {
			"default": "./src/proxy/*"
		},
		"#common/*": {
			"default": "./src/common/*"
		}
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"dotenv-flow": "^4.0.0",
		"find-package-json": "^1.2.0",
		"fs-extra": "^11.1.1",
		"globby": "^13.2.2",
		"http-proxy-middleware": "^2.0.6",
		"json-stringify-safe": "^5.0.1",
		"micromatch": "^4.0.5",
		"triple-beam": "^1.4.1",
		"vite": "^4.4.11",
		"vitest": "^0.34.6",
		"winston": "^3.11.0",
		"yaml": "^2.3.2",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@vitest/coverage-v8": "^0.34.6",
		"typescript-transform-paths": "^3.4.6",
		"va-eslint-config": "^1.0.17"
	}
}
